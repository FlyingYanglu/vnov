        "audio_url": "https://v3-vvecloud.yangyi08.com/6e82dede2285897fcc456da88dcab02c/6724c6d2/video/tos/cn/tos-cn-v-2cd6b0/oUAkAA175QvkwyCiS8Mq0uIuBAEZwiD4OoIAE/?a=7073&ch=0&cr=0&dr=0&&br=0&bt=0&ft=xztlUQhhe6BMyqqIUupJD12Nzj&mime_type=audio_mpeg&rc=QGlAYSNz&btag=c0000000008000&dy_q=1730459842&l=202411011917131EB5EC2E40DC706B4524&format=mp3",
        "source_timerange": {
            "start_offset": 0,
            "end_offset": 3.0999999999999943
        },
        "target_timerange": {
            "start_offset": 134.78,
            "end_offset": 137.88
        },
        "ignore_audio": false,
        "audio_path": null
    },
