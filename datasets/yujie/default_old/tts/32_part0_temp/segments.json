        "audio_url": "https://v26-vvecloud.yangyi08.com/4b195b22243c25d07ca456dfeab760b9/6724c6d2/video/tos/cn/tos-cn-v-2cd6b0/o4CAJFxCjJUYDceARfsBLUilgDEdABAItnz6Io/?a=7073&ch=0&cr=0&dr=0&&br=0&bt=0&ft=xztlUQhhe6BMyqqIUupJD12Nzj&mime_type=audio_mpeg&rc=QGlAYSNz&btag=c0000000008000&dy_q=1730459842&l=202411011917131EB5EC2E40DC706B4524&format=mp3",
        "source_timerange": {
            "start_offset": 0,
            "end_offset": 2.8400000000000034
        },
        "target_timerange": {
            "start_offset": 127.37,
            "end_offset": 130.21
        },
        "ignore_audio": false,
        "audio_path": null
    },
