        "audio_url": "https://v9-vvecloud.yangyi08.com/c32238a61c8927d62a2778c1a91e494d/6724c6d2/video/tos/cn/tos-cn-v-2cd6b0/owAAbcuAwiEMALhkwZBIIQv4dk1yAS1qoiIEW/?a=7073&ch=0&cr=0&dr=0&&br=0&bt=0&ft=xztlUQhhe6BMyqqIUupJD12Nzj&mime_type=audio_mpeg&rc=QGlAYSNz&btag=c0000000008000&dy_q=1730459842&l=202411011917131EB5EC2E40DC706B4524&format=mp3",
        "source_timerange": {
            "start_offset": 0,
            "end_offset": 3.0999999999999943
        },
        "target_timerange": {
            "start_offset": 137.88,
            "end_offset": 140.98
        },
        "ignore_audio": false,
        "audio_path": null
    },
