        "audio_url": "https://v26-vvecloud.yangyi08.com/810fdbc6849c390444543dab38bfaf51/6724c6d2/video/tos/cn/tos-cn-v-2cd6b0/oILiiJAASFQmCBDALneCRagAjEEbfAnDEkuMI5/?a=7073&ch=0&cr=0&dr=0&&br=0&bt=0&ft=xztlUQhhe6BMyqqIUupJD12Nzj&mime_type=audio_mpeg&rc=QGlAYSNz&btag=c0000000008000&dy_q=1730459842&l=202411011917131EB5EC2E40DC706B4524&format=mp3",
        "source_timerange": {
            "start_offset": 0,
            "end_offset": 2.5600000000000023
        },
        "target_timerange": {
            "start_offset": 143.75,
            "end_offset": 146.31
        },
        "ignore_audio": false,
        "audio_path": null
    },
    {
