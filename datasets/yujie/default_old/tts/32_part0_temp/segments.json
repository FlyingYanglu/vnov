        "audio_url": "https://v6-vvecloud.yangyi08.com/c773ea8f8ebc390c030b11a84c7c25a4/6724c6d2/video/tos/cn/tos-cn-v-2cd6b0/o0dkC2jEAGRfAELEXBFnCuADgWDnAAPMfLJiv4/?a=7073&ch=0&cr=0&dr=0&&br=0&bt=0&ft=xztlUQhhe6BMyqqIUupJD12Nzj&mime_type=audio_mpeg&rc=QGlAYSNz&btag=c0000000008000&dy_q=1730459842&l=202411011917131EB5EC2E40DC706B4524&format=mp3",
        "source_timerange": {
            "start_offset": 0,
            "end_offset": 4.569999999999993
        },
        "target_timerange": {
            "start_offset": 130.21,
            "end_offset": 134.78
        },
        "ignore_audio": false,
        "audio_path": null
